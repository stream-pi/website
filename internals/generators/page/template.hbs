{{/if}}
 = (
  {{#if wantGetServerSideProps}}
  _props
  {{/if}}
  {{#if wantGetStaticProps}}
  _props
  {{/if}}
) => {
  return (
    <>
      <NextSeo title="{{ properCase pageName }}" />
      <h1 style={ {textAlign: "center"} }>{{ properCase pageName }}</h1>
