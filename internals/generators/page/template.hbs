{{/if}}
export const getServerSideProps: GetServerSideProps<
  ServerProps,
  {{#if dynamicRoute}}
  ServerParams
  {{/if}}
> = async (_context) => {
  {{#if dynamicRoute}}
  console.log(_context.params?.{{ snakeCase pageName }});
  {{/if}}
  return { props: { foo: "hello" } };
