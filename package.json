{
  "name": "streampi-website",
  "version": "2.1.0",
  "author": "Samuel Qui√±ones",
  "description": "The official Stream-Pi website",
  "license": "GPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/stream-pi/website"
  },
  "bugs": {
    "url": "https://github.com/stream-pi/website/issues"
  },
  "config": {
    "static_out": "out"
  },
  "scripts": {
    "dev": "next dev -p 4000",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' next dev -p 4000",
    "build": "next build",
    "build:full": "npm run build && npm run export",
    "export": "cross-env-shell \"next export -o $npm_package_config_static_out\"",
    "start": "next start -p 8080",
    "start:dev": "next start -p 3012",
    "start:static": "cross-env-shell \"serve $npm_package_config_static_out\"",
    "eslint": "eslint --ext js,ts,tsx",
    "stylelint": "stylelint \"src/assets/styles/**/*.{css,scss,sass}\"",
    "lint:next": "next lint -d src",
    "lint": "npm run eslint src && npm run stylelint",
    "lint:fix": "npm run eslint -- --fix src && npm run stylelint -- --fix",
    "pretty:check": "prettier --check .",
    "pretty:fix": "prettier --write .",
    "checkTs": "tsc --noEmit",
    "update-release:server": "node ./.scripts/updateRecentRelease.js server && prettier --write ./src/modules/Install/Helper/server.ts",
    "update-release:client": "node ./.scripts/updateRecentRelease.js client && prettier --write ./src/modules/Install/Helper/client.ts",
    "update-release:all": "npm run update-release:server && npm run update-release:client",
    "preinstall": "node ./.scripts/nodeVersionCheck.js && node ./.scripts/npmVersionCheck.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "@hookform/resolvers": "2.8.5",
    "@reduxjs/toolkit": "1.7.1",
    "@restart/ui": "0.2.5",
    "animate.css": "4.1.1",
    "axios": "0.24.0",
    "bootstrap": "5.1.3",
    "classnames": "2.3.1",
    "dayjs": "1.10.7",
    "next": "12.0.7",
    "nodemailer": "6.7.2",
    "react": "17.0.2",
    "react-bootstrap": "2.1.0",
    "react-dom": "17.0.2",
    "react-google-recaptcha": "2.1.0",
    "react-hook-form": "7.22.5",
    "react-redux": "7.2.6",
    "react-spinners": "0.11.0",
    "react-toastify": "7.0.4",
    "sass": "1.44.0",
    "sharp": "0.29.3",
    "use-dark-mode": "2.3.1",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@types/node": "16.11.11",
    "@types/nodemailer": "6.4.4",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-google-recaptcha": "2.1.2",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "cross-env": "7.0.3",
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "prettier": "2.5.1",
    "serve": "13.0.2",
    "stylelint": "14.1.0",
    "stylelint-config-recommended": "6.0.0",
    "stylelint-config-standard-scss": "3.0.0",
    "typescript": "4.3.5"
  }
}
